{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current",
            "browsers": [
              ">1%",
              "not ie 11"
            ]
          },
          "loose": true,
          "bugfixes": true,
          "modules": false,
          "forceAllTransforms": true,
          "corejs": 3,
          "useBuiltIns": "entry"
        }
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ]
}